<?xml version="1.0" encoding="UTF-8"?>
<fdframework-business xmlns="http://www.neusoft.com/fdframework/business"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<business id="pfriskitemproperty.getPsItemDef" returnkey="getPsItemDef" name="" desc="获取险种保全下有公式的保全项">
		<step id="1" bean="factoryabclife_pfRiskItemPropertyBo_bo" method="getPsItemDef" returnkey="getPsItemDef">
			<property refkey="insurtypeId" type="long" ></property>
		</step>
	</business>
	
	<business id="pfriskitemproperty.savePsItemDefs" returnkey="" name="" desc="保存有公式的保全">
		<step id="1" bean="factoryabclife_pfRiskItemPropertyBo_bo" method="savePsItemDefs" returnkey="">
			<property refkey="tInsurtypePsItemDefs" type="pojoList(com.neusoft.abclife.productfactory.entity.TInsurtypePsItemDef)"></property>
			<property refkey="insurtype" type="pojo(com.neusoft.abclife.productfactory.entity.TInsurtypeBasicInf)"></property>
		</step>
	</business>
	
	<business id="pfriskitemproperty.getPsItemchange" returnkey="getPsItemchange" desc="获取转义用的信息">
		<step id="1" bean="factoryabclife_pfRiskItemPropertyBo_bo" method="getPsItemchange" returnkey="getPsItemchange">
		</step>
	</business>
	
</fdframework-business>
