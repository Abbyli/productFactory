<?xml version="1.0" encoding="UTF-8"?>
<fdframework-business xmlns="http://www.neusoft.com/fdframework/business"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<business id="pfriskparam.queryDimensionDef" returnkey="queryDimensionDefResult"
		name="" desc="维度定义查询">
		<step id="1" bean="factoryabclife_pfDimensionDefBo_bo" method="queryDimensionDef"
			returnkey="queryDimensionDefResult">
			<property refkey="dimensionDef"
				type="pojo(com.neusoft.abclife.productfactory.entity.TDimensionDef)"></property>
			<property refkey="pageNumber" type="int"></property>
			<property refkey="pageSize" type="int"></property>

		</step>
	</business>

	<business id="pfriskparam.saveDimensionDef" returnkey="saveDimensionDefResult"
		name="" desc="维度定义保存">
		<step id="1" bean="factoryabclife_pfDimensionDefBo_bo" method="saveDimensionDef"
			returnkey="saveDimensionDefResult">
			<property refkey="dimensionDef"
				type="pojo(com.neusoft.abclife.productfactory.entity.TDimensionDef)"></property>
			<property refkey="opt" type="string"></property>
		</step>
	</business>

	<business id="pfriskparam.delDimensionDef" returnkey="delDimensionDefResult"
		name="" desc="维度定义删除">
		<step id="1" bean="factoryabclife_pfDimensionDefBo_bo" method="delDimensionDef"
			returnkey="delDimensionDefResult">
			<property refkey="dimensionDef"
				type="pojo(com.neusoft.abclife.productfactory.entity.TDimensionDef)"></property>
		</step>
	</business>
	
	<business id="pfriskparam.queryDimensionDefNoPage" returnkey="queryDimensionDefNoPage"
		name="" desc="维度查询非翻页">
		<step id="1" bean="factoryabclife_pfDimensionDefBo_bo" method="queryDimensionDefNoPage"
			returnkey="queryDimensionDefNoPage">
			<property refkey="dimensionDef"
				type="pojo(com.neusoft.abclife.productfactory.entity.TDimensionDef)"></property>
		</step>
	</business>
	
	<business id="pfriskparam.changeOrderNum" returnkey="changeOrderNum"
		name="" desc="维度查询非翻页">
		<step id="1" bean="factoryabclife_pfDimensionDefBo_bo" method="changeOrderNum"
			returnkey="changeOrderNum">
			<property refkey="dimensionDef1"
				type="pojo(com.neusoft.abclife.productfactory.entity.TDimensionDef)"></property>
				<property refkey="dimensionDef2"
				type="pojo(com.neusoft.abclife.productfactory.entity.TDimensionDef)"></property>
		</step>
	</business>

</fdframework-business>