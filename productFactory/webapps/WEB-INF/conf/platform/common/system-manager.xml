<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
<struts>
	<package name="struts-fdframework-manager" extends="struts-fdframework">
		<action name="commonManageAction_*" class="com.neusoft.fdframework.web.action.CommonManageAction" method="{1}">
            <result name="loadcode" type="stream">      
            	<param name="contentType">image/jpeg</param>      
           		 <param name="inputName">inputStream</param>      
       		</result>
        </action>
       <action name="fileUploadAction_*" class="com.neusoft.fdframework.web.action.FileUploadAction" method="{1}">
            <result name="loadcode" type="stream">      
            	<param name="contentType">image/jpeg</param>      
           		<param name="inputName">inputStream</param>      
       		</result>
        </action>
         <action name="uploadFileAction_*" class="com.neusoft.fdframework.web.action.FileUploadAction" method="{1}">
            <result name="loadcode" type="stream">      
            	<param name="contentType">image/jpeg</param>      
           		 <param name="inputName">inputStream</param>      
       		</result>
        </action>
	</package>
</struts>