<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping> 
	<class name="com.neusoft.abclife.productfactory.entity.TOccRate" table="T_OCC_RATE">
		<id name="id" type="java.lang.Long">
			<column length="16" name="ID"/>
			<generator class="sequence">
				<param name="sequence"></param>
			</generator>
		</id>
		<property name="payintv" type="big_decimal">
			<column length="22" name="PAYINTV" not-null="true"/>
		</property>
		<property name="occupationtype" type="big_decimal">
			<column length="22" name="OCCUPATIONTYPE" not-null="true"/>
		</property>
		<property name="suppriskscore" type="big_decimal">
			<column length="8" name="SUPPRISKSCORE" not-null="true"/>
		</property>
	</class>
</hibernate-mapping>
	