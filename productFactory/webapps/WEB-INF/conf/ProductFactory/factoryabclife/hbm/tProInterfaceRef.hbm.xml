<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping> 
	<class name="com.neusoft.abclife.productfactory.entity.TProInterfaceRef" table="T_PRO_INTERFACE_REF">
		<id name="proId" type="java.lang.Long">
			<column length="16" name="PRO_ID"/>
			<generator class="sequence"/>
		</id>
		<property name="proCode" type="string">
			<column length="10" name="PRO_CODE" not-null="false"/>
		</property>
		<property name="proName" type="string">
			<column length="100" name="PRO_NAME" not-null="false"/>
		</property>
		<property name="proType" type="string">
			<column length="2" name="PRO_TYPE" not-null="false"/>
		</property>
		<property name="proStatus" type="string">
			<column length="2" name="PRO_STATUS" not-null="false"/>
		</property>
	</class>
</hibernate-mapping>
	