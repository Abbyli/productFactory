<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping> 
	<class name="com.neusoft.abclife.productfactory.entity.TComboAttrib" table="T_COMBO_ATTRIB">
		<id name="comboAttribId" type="java.lang.Long">
			<column length="16" name="COMBO_ATTRIB_ID"/>
			<generator class="sequence">
				<param name="sequence">SEQ_COMBO_ATTRIB</param>
			</generator>
		</id>
		<property name="comboId" type="java.lang.Long">
			<column length="16" name="COMBO_ID" not-null="false"/>
		</property>
		<property name="comboSalesWay" type="string">
			<column length="2" name="COMBO_SALES_WAY" not-null="false"/>
		</property>
		<property name="isByCopiesSales" type="string">
			<column length="1" name="IS_BY_COPIES_SALES" not-null="false"/>
		</property>
		<property name="bonusToAccFlag" type="string">
			<column length="2" name="BONUS_TO_ACC_FLAG" not-null="false"/>
		</property>
		<property name="isPermitAutoPay" type="string">
			<column length="2" name="IS_PERMIT_AUTO_PAY" not-null="false"/>
		</property>
		<property name="isPermitInsurRenew" type="string">
			<column length="2" name="IS_PERMIT_INSUR_RENEW" not-null="false"/>
		</property>
		<property name="liveToAccFlag" type="string">
			<column length="2" name="LIVE_TO_ACC_FLAG" not-null="false"/>
		</property>
		<property name="isHaveIndependRate" type="string">
			<column length="1" name="IS_HAVE_INDEPEND_RATE" not-null="false"/>
		</property>
		<property name="premAlgoId" type="java.lang.Long">
			<column length="16" name="PREM_ALGO_ID" not-null="false"/>
		</property>
		<property name="suminsurAlgoId" type="java.lang.Long">
			<column length="16" name="SUMINSUR_ALGO_ID" not-null="false"/>
		</property>
	</class>
</hibernate-mapping>
	