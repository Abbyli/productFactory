<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping> 
	<class name="com.neusoft.abclife.productfactory.entity.TSkelementDef" table="T_SKELEMENT_DEF">
		<id name="id" type="java.lang.Long">
			<column length="16" name="ID"/>
			<generator class="sequence">
				<param name="sequence">SEQ_SKELEMENT_DEF</param>
			</generator>
		</id>
		<property name="name" type="string">
			<column length="300" name="NAME" not-null="false"/>
		</property>
		<property name="isCalRef" type="string">
			<column length="2" name="IS_CAL_REF" not-null="false"/>
		</property>
		<property name="property" type="string">
			<column length="64" name="PROPERTY" not-null="false"/>
		</property>
		<property name="busiType" type="string">
			<column length="2" name="BUSI_TYPE" not-null="false"/>
		</property>
	</class>
</hibernate-mapping>
	