<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping> 
	<class name="com.neusoft.abclife.productfactory.entity.TDimensionDef" table="T_DIMENSION_DEF">
		<id name="id" type="java.lang.Long">
			<column length="16" name="ID"/>
			<generator class="sequence">
				<param name="sequence">SEQ_DIMENSION_DEF</param>
			</generator>
		</id>
		<property name="name" type="string">
			<column length="20" name="NAME" not-null="false"/>
		</property>
		<property name="property" type="string">
			<column length="20" name="PROPERTY" not-null="false"/>
		</property>
		<property name="columnType" type="string">
			<column length="20" name="COLUMN_TYPE" not-null="false"/>
		</property>
		<property name="orderNum" type="java.lang.Integer">
			<column length="6" name="ORDER_NUM" not-null="false"/>
		</property>
		<property name="matchFlag" type="string">
			<column length="2" name="MATCH_FLAG" not-null="false"/>
		</property>
		<property name="dimensionType" type="string">
			<column length="2" name="DIMENSION_TYPE" not-null="false"/>
		</property>
		<property name="returnType" type="string">
			<column length="20" name="RETURN_TYPE" not-null="false"/>
		</property>
		<property name="refValue" type="string">
			<column length="255" name="REF_VALUE" not-null="false"/>
		</property>
		<property name="rateType" type="string">
			<column length="6" name="RATE_TYPE" not-null="false"/>
		</property>
	</class>
</hibernate-mapping>
	