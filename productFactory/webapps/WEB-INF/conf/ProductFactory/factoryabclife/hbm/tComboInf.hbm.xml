<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping> 
	<class name="com.neusoft.abclife.productfactory.entity.TComboInf" table="T_COMBO_INF">
		<id name="comboId" type="java.lang.Long">
			<column length="16" name="COMBO_ID"/>
			<generator class="sequence">
				<param name="sequence">SEQ_COMBO_INF</param>
			</generator>
		</id>
		<property name="comboCode" type="string">
			<column length="20" name="COMBO_CODE" not-null="false"/>
		</property>
		<property name="comboVer" type="java.lang.Long">
			<column length="16" name="COMBO_VER" not-null="false"/>
		</property>
		<property name="comboStatu" type="string">
			<column length="2" name="COMBO_STATU" not-null="false"/>
		</property>
		<property name="comboName" type="string">
			<column length="120" name="COMBO_NAME" not-null="false"/>
		</property>
		<property name="comboAbbr" type="string">
			<column length="90" name="COMBO_ABBR" not-null="false"/>
		</property>
		<property name="remark" type="string">
			<column length="500" name="REMARK" not-null="false"/>
		</property>
	</class>
</hibernate-mapping>
	