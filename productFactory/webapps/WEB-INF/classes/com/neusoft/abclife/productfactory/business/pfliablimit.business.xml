<?xml version="1.0" encoding="UTF-8"?>
<fdframework-business xmlns="http://www.neusoft.com/fdframework/business"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<business id="pfliablimit.saveLiabLimit" name="" returnkey="saveLiabLimit" desc="保存责任限额">
		<step id="1" bean="factoryabclife_pfRiskLiabLimitBo_bo" method="saveLiabLimit" returnkey="saveLiabLimit">
			<property refkey="tLiabLimit" type="com.neusoft.abclife.productfactory.entity.TLiabLimit"></property>
		</step>
	</business>
	
	<business id="pfliablimit.queryTLiabLimit" name="" returnkey="queryTLiabLimit" desc="保存责任限额">
		<step id="1" bean="factoryabclife_pfRiskLiabLimitBo_bo" method="queryTLiabLimit" returnkey="queryTLiabLimit">
			<property refkey="protecLiabCode" type="string"></property>
			<property refkey="limitType" type="string"></property>
			<property refkey="riskVer" type="long"></property>
		</step>
	</business>
	
	<business id="pfliablimit.delTLiabLimit" name="" returnkey="delTLiabLimit" desc="保存责任限额">
		<step id="1" bean="factoryabclife_pfRiskLiabLimitBo_bo" method="delTLiabLimit" returnkey="delTLiabLimit">
			<property refkey="tLiabLimit" type="com.neusoft.abclife.productfactory.entity.TLiabLimit"></property>
		</step>
	</business>
	
</fdframework-business>