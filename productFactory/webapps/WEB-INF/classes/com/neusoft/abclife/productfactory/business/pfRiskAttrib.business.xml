<?xml version="1.0" encoding="UTF-8"?>
<fdframework-business xmlns="http://www.neusoft.com/fdframework/business"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<business id="pfRiskAttrib.queryAttrib" returnkey="queryAttribResult"
		name="" desc="保全属性查询">
		<step id="1" bean="factoryabclife_pfRiskAttribBo_bo" method="queryAttrib"
			returnkey="queryAttribResult">
			<property refkey="basic"
				type="pojo(com.neusoft.abclife.productfactory.entity.TInsurtypeBasicInf)"></property>
		</step>
	</business>
	
	<business id="pfRiskAttrib.saveAttrib" returnkey="saveAttribResult"
		name="" desc="保全属性保存">
		<step id="1" bean="factoryabclife_pfRiskAttribBo_bo" method="saveAttrib"
			returnkey="saveAttribResult">
			<property refkey="basic"
				type="pojo(com.neusoft.abclife.productfactory.entity.TInsurtypeBasicInf)"></property>
			<property refkey="attrib"
				type="pojo(com.neusoft.abclife.productfactory.entity.TInsurtypePsAttribDef)"></property>
		</step>
	</business>
</fdframework-business>
