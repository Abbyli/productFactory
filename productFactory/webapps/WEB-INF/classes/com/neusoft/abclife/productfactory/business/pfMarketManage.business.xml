<?xml version="1.0" encoding="UTF-8"?>
<fdframework-business xmlns="http://www.neusoft.com/fdframework/business"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	
	<business id="pfMarketManage.queryTProductSaleChnl" returnkey="reTProductSaleChnl"
		name="" desc="产品查询">
		<step id="1" bean="factoryabclife_pfMarketManageBo_bo" method="queryTProductSaleChnl"
			returnkey="reTProductSaleChnl">
			<property refkey="tProductSaleChnl"
				type="pojo(com.neusoft.abclife.productfactory.dto.TProductSaleChnl)"></property>
			<property refkey="pageNumber" type="int"></property>
			<property refkey="pageSize" type="int"></property>
		</step>
	</business>
	<business id="pfMarketManage.queryTInsurtypeBasicInf" returnkey="reTInsurtypeBasicInf"
		name="" desc="在售险种查询">
		<step id="1" bean="factoryabclife_pfMarketManageBo_bo" method="queryTInsurtypeBasicInf"
			returnkey="reTInsurtypeBasicInf">
		</step>
	</business>
	<business id="pfMarketManage.addTProductSaleChnl" returnkey="reAddTProductSaleChnl"
		name="" desc="产品销售控制新增或修改">
		<step id="1" bean="factoryabclife_pfMarketManageBo_bo" method="addTProductSaleChnl"
			returnkey="reAddTProductSaleChnl">
			<property refkey="tProductSaleChnl"
				type="pojo(com.neusoft.abclife.productfactoty.entity.TProductSaleChnl)"></property>
			<property refkey="opt" type="String"></property>
		</step>
	</business>
	<business id="pfMarketManage.upTProductSaleChnl" returnkey="reUpTProductSaleChnl"
		name="" desc="产品销售控制新增或修改">
		<step id="1" bean="factoryabclife_pfMarketManageBo_bo" method="upTProductSaleChnl"
			returnkey="reUpTProductSaleChnl">
			<property refkey="tProductSaleChnl"
				type="pojo(com.neusoft.abclife.productfactoty.entity.TProductSaleChnl)"></property>
		</step>
	</business>
	
	
</fdframework-business>
