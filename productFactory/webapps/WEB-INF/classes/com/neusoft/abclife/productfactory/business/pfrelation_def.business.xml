<?xml version="1.0" encoding="UTF-8"?>
<fdframework-business xmlns="http://www.neusoft.com/fdframework/business"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

	<business id="pfriskparam.queryRelationDef" returnkey="queryRelationDefResult"
		name="" desc="相关性定义查询">
		<step id="1" bean="factoryabclife_pfRelationDefBo_bo" method="queryRelationDef"
			returnkey="queryRelationDefResult">
			<property refkey="pageNumber" type="int"></property>
			<property refkey="pageSize" type="int"></property>

		</step>
	</business>

	<business id="pfriskparam.saveRelationDef" returnkey="saveRelationDefResult"
		name="" desc="相关性定义保存">
		<step id="1" bean="factoryabclife_pfRelationDefBo_bo" method="saveRelationDef"
			returnkey="saveRelationDefResult">
			<property refkey="relationDef"
				type="pojo(com.neusoft.abclife.productfactory.entity.TRelationDef)"></property>
			<property refkey="opt" type="string"></property>
		</step>
	</business>

	<business id="pfriskparam.delRelationDef" returnkey="delRelationDefResult"
		name="" desc="相关性定义保存">
		<step id="1" bean="factoryabclife_pfRelationDefBo_bo" method="delRelationDef"
			returnkey="delRelationDefResult">
			<property refkey="relationDef"
				type="pojo(com.neusoft.abclife.productfactory.entity.TRelationDef)"></property>
		</step>
	</business>

</fdframework-business>