if(returnKey)
{
	var name=unieap.byId("name").getValue();
	var condition=unieap.byId("condition").getValue();
	ifCondition_sel.pushHistory({content:condition,name:name});
	condition=condition+"#"+returnKey;
	unieap.byId("condition").setValue(condition);
	
	
	var returnName=unieap.byId("returnKey").getText();
	name=name+returnName;
	unieap.byId("name").setValue(name);
}


]]></script>
