		<event name="onclick" handlerFunction="img2_onclick">
			<function name="img2_onclick" arguments="">
				<script><![CDATA[var bomArributeObj=unieap.byId("property");
var bomAttribute;

var attributeStore=dataCenter.getDataStore("attributeStore");
if(attributeStore)
{
	if(attributeStore&&attributeStore.getRowSet().getRowCount(0)>0)
	{
		for(var i=0,n=attributeStore.getRowSet().getRowCount(0);i<n;i++)
		{
			var id=attributeStore.getRowSet().getRow(i).getItemValue("id");
			var property=attributeStore.getRowSet().getRow(i).getItemValue("property");
			if(id==bomArributeObj.getValue())
			{
				bomAttribute=property;
				break;
			}
		}
	}
}
		

if(bomAttribute)
{
	var name=unieap.byId("name").getValue();
	var condition=unieap.byId("condition").getValue();
	ifCondition_sel.pushHistory({content:condition,name:name});
	var bomProperty=unieap.byId("returnKey").getValue();
	condition=condition+"#"+bomProperty+"."+bomAttribute;
	unieap.byId("condition").setValue(condition);
	
	
	
	
	var returnName=unieap.byId("returnKey").getText();
	var bomAttributeName=unieap.byId("property").getText();
	name=name+returnName+"."+bomAttributeName;
	unieap.byId("name").setValue(name);
}

]]></script>
			</function>
		</event>
	</events>
