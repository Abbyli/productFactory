  </template><events><event name="load" handlerFunction="page_load"><function name="page_load" arguments=""><script><![CDATA[unieap.byId("property").setDisplay("none");

unieap.getElementById("img2").style.display='none';
unieap.getElementById("property_label").style.display='none';

unieap.byId("returnType").setValue("string");
ifCondition_sel.historyStack=new Array();

unieap.byId("fixValue").setDisplay("");
unieap.byId("numberTextBox1").setDisplay("none");

var ruleContext=null;
var dialogDs=unieap.getXDialog().dialogData;
if(dialogDs.data !=null){
	ruleContext= dialogDs.data;
	paramStore=dataCenter.getDataStore("paramStore");
	var paraArray=ruleContext.getParamList();
	if(paraArray&&paraArray.length>0)
	{
		for(var i=0,n=paraArray.length;i<n;i++)
		{
			paramStore.getRowSet().addRow(paraArray[i]);
		}
	}
}


]]></script></function></event><event name="init" handlerFunction="page_init"><function name="page_init" arguments=""><script><![CDATA[var paramStore=new unieap.ds.DataStore("paramStore");
var attributeStore=new unieap.ds.DataStore("attributeStore");

var ifTypeStore=new unieap.ds.DataStore("ifTypeStore");
ifTypeStore.getRowSet().addRow({id:'string',name:'字符串'});
ifTypeStore.getRowSet().addRow({id:'number',name:'数值型'});

dataCenter.addDataStore("ifTypeStore", ifTypeStore);
dataCenter.addDataStore("paramStore", paramStore);
dataCenter.addDataStore("attributeStore", attributeStore);]]></script></function></event></events></controls>
      <scripts ><function name="getBomAttributeSuccess"
	arguments="dc">
	<comment><![CDATA[/**
 * @description:getBomAttribute方法的成功回调。
 * 
 */
]]></comment>
	<script><![CDATA[try{
	var store = dc.getDataStore("result");
	if(store){
		var attributeStore=dataCenter.getDataStore("attributeStore");
		if(attributeStore)
		{
			var array=[];
			if(store&&store.getRowSet().getRowCount(0)>0)
			{
				for(var i=0,n=store.getRowSet().getRowCount(0);i<n;i++)
				{
					var id=store.getRowSet().getRow(i).getItemValue("id");
					var name=store.getRowSet().getRow(i).getItemValue("name");
					var parentId=store.getRowSet().getRow(i).getItemValue("parentId");
					var property=store.getRowSet().getRow(i).getItemValue("property");
					array.push({id:id,name:name,parentId:parentId,property:property});
				}
			}
			attributeStore.getRowSet().deleteAllRows();
			attributeStore.getRowSet().addRows(array);
		}
		else
		{
			dataCenter.addDataStore("attributeStore",store);
		}
		var treeJson={
			binding:{leaf:'leaf',store:'attributeStore',id:'id',parent:'parentId',label:'name',query:{name:'parentId',relation:'=',value:null}}
		};
		unieap.byId("property").setTreeJson(treeJson);
	}
}
catch (e){
}
]]></script>
</function><function name="validConditionSuccess" arguments="dc">
	<comment><![CDATA[/**
 * @description:validCondition方法的成功回调。
 * 
 */
]]></comment>
	<script><![CDATA[var code = dc.getParameter("result");

if(code>0)
{
	var retObj={condition:unieap.byId("condition").getValue(),name:unieap.byId("name").getValue()};
	unieap.getXDialog().setReturn(retObj);
	unieap.getXDialog().close();
	
}
else
{
	MessageBox.alert({
        title: '提示信息',
        message: "表达式不合法，请重新输入。",
        onComplete: function () {
		}
	});
}
]]></script>
</function><function name="pushHistory" arguments="contentObj">
	<comment><![CDATA[/**
 * @description:
 * 
* @param: {参数类型} arg0参数描述 
 * @return:
 * 
 */]]></comment>
	<script><![CDATA[ifCondition_sel.historyStack.push(contentObj);]]>
	</script>
</function><function name="popHistory" arguments="">
	<comment><![CDATA[/**
 * @description:
 * 
* @param: {参数类型} arg0参数描述 
 * @return:
 * 
 */]]></comment>
	<script><![CDATA[if(ifCondition_sel.historyStack.length>0)
{
	return ifCondition_sel.historyStack.pop();
}
return {content:"",name:""};]]>
	</script>
</function></scripts>
